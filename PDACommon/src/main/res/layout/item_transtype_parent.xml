<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data class="DialogTranstypeAdapterParentBinding">

        <variable
            name="bean"
            type="com.jd.saas.pdacommon.dialog.linkage.model.ParentTansType" />
    </data>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/parentTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:paddingHorizontal="25dp"
        android:paddingVertical="15dp"
        android:text="@{bean.name}"
        android:textColor="@color/color_dialog_linkage_black"
        android:textSize="@dimen/text_normal"
        app:backgroundResource="@{bean.backgroundRes}"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</layout>