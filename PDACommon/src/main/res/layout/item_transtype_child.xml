<?xml version="1.0" encoding="utf-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <data class="DialogTranstypeAdapterChildBinding">

        <variable
            name="bean"
            type="com.jd.saas.pdacommon.dialog.linkage.model.ChildTansType" />
    </data>

    <androidx.appcompat.widget.AppCompatTextView
        android:id="@+id/childTv"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginHorizontal="10dp"
        android:layout_marginBottom="10dp"
        android:background="@drawable/selector_dialog_linkage_trance_type_bg"
        android:paddingHorizontal="20dp"
        android:paddingVertical="15dp"
        android:text="@{bean.name}"
        android:textColor="@drawable/selector_dialog_linkage_trance_type_text_color"
        android:textSize="@dimen/text_normal"
        app:layout_constraintLeft_toLeftOf="parent"
        app:layout_constraintTop_toTopOf="parent" />
</layout>